		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"@lucide/svelte": "^0.562.0",
